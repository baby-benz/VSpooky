{
	"name": "vspooky",
	"displayName": "VSpooky",
	"description": "Spooky Spoty Player",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"private": true,
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"prebuild": "npm install",
		"webpack": "webpack --mode development",
		"webpack-watch": "webpack --mode development --watch",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "tsc ./src/test/runTest.ts"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.7.3",
		"vsce": "^1.71.0",
		"vscode-test": "^1.3.0",
		"webpack": "^4.41.3",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"spotify-web-api-js": "^1.2.0"
	}
}
